{
  "manifest_version": 3,
  "name": "New Tab Vision Pro",
  "version": "1.0.0",
  "action": {
    "default_icon": "icon.png"
  },
  "description": "Customize new tab as Vison OS style.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "history", 
    "management", 
    "bookmarks", 
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "worker.js"
  }
}